#include <iostream>
#include <fstream>

char encrypt(char c, char key){
	return c ^ key;
}


int main(int argc, char *argv[]){
	std::ifstream r (argv[2]);
	std::ofstream o (argv[3]);

	for(int i = 0; i < argc; i++){
		std::cout << i << ' '  << argv[i] << std::endl;
	}

	char c, key;
	key = argv[1][0];
	while(!r.eof()){
		r.get(c);
		o.put(encrypt(c, key));
	}	

	return 0;
}


